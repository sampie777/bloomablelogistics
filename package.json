{
  "name": "bloomablelogistics",
  "version": "0.7.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bundle": "cd android && ./gradlew bundleRelease",
    "build": "cd android && ./gradlew assembleRelease",
    "device-install": "adb -d install \"$(ls -t ./android/app/build/outputs/apk/release/*.apk | head -1)\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-clipboard/clipboard": "^1.11.1",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.0",
    "deprecated-react-native-prop-types": "^2.3.0",
    "html-entities": "^2.3.3",
    "react": "18.2.0",
    "react-native": "0.70.3",
    "react-native-cache": "^2.0.2",
    "react-native-config": "^1.4.11",
    "react-native-device-info": "^10.3.0",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-geocoding": "^0.5.0",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-maps": "^1.3.2",
    "react-native-reanimated": "^2.11.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0",
    "recoil": "^0.7.6",
    "rollbar-react-native": "^0.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/runtime": "^7.19.4",
    "@react-native-community/eslint-config": "^3.1.0",
    "@types/jest": "^29.2.0",
    "@types/react-native": "^0.70.6",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-jest": "^29.2.1",
    "eslint": "^8.25.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "^0.73.2",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/testSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(rollbar-react-native)"
    ],
    "modulePathIgnorePatterns": [
      "__tests__/resources"
    ]
  }
}
